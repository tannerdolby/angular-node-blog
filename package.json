{
  "name": "angular-node-blog-template",
  "version": "1.0.0",
  "description": "Node.js + Angular blog template prepared for Netlify.",
  "main": "server.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "netlify-lambda": "^2.0.3",
    "nodemon": "^2.0.7",
    "serverless-http": "^2.7.0"
  },
  "devDependencies": {},
  "scripts": {
    "build": "cd blog-client && ng build --prod && cd node-app && netlify-lambda build src",
    "start": "node ./src/app.js",
    "dev": "nodemon ./src/app.js",
    "serve": "netlify-lambda serve src",
    "start-l": "netlify-lambda build src"
  },
  "keywords": [
    "netlify",
    "angular",
    "node.js",
    "blog-template"
  ],
  "author": "Tanner Dolby",
